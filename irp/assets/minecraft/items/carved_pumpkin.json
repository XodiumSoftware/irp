{
  "model": {
    "type": "minecraft:condition",
    "property": "minecraft:has_component",
    "component": "minecraft:custom_name",
    "on_true": {
      "type": "minecraft:select",
      "model": "minecraft:block/carved_pumpkin",
      "property": "minecraft:component",
      "component": "minecraft:custom_name",
      "cases": [
        {
          "when": "Hood",
          "model": {
            "type": "minecraft:model",
            "model": "minecraft:item/excalibur/carved_pumpkin/hood"
          }
        },
        {
          "when": "Jar",
          "model": {
            "type": "minecraft:model",
            "model": "minecraft:item/excalibur/carved_pumpkin/jar"
          }
        },		
        {
          "when": "Red Headband",
          "model": {
            "type": "minecraft:model",
            "model": "minecraft:item/excalibur/carved_pumpkin/headband_red"
          }
        },		
        {
          "when": "Straw Hat",
          "model": {
            "type": "minecraft:model",
            "model": "minecraft:item/excalibur/carved_pumpkin/straw_hat"
          }
        },		
        {
          "when": "Wizard Hat",
          "model": {
            "type": "minecraft:model",
            "model": "minecraft:item/excalibur/carved_pumpkin/wizard_hat"
          }
        }
      ],
      "fallback": {
        "type": "minecraft:model",
        "model": "minecraft:block/carved_pumpkin"
      }
    },
    "on_false": {
      "type": "minecraft:select",
      "property": "minecraft:display_context",
      "cases": [
        {
          "model": {
            "type": "minecraft:model",
            "model": "item/excalibur/carved_pumpkin/carved_pumpkin"
          },
          "when": [
            "head"
          ]
        }
      ],
      "fallback": {
        "type": "minecraft:model",
        "model": "minecraft:block/carved_pumpkin"
      }
    }
  }
}