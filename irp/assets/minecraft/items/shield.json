{
  "model": {
    "type": "minecraft:condition",
    "on_false": {
      "type": "minecraft:condition",
      "property": "minecraft:has_component",
      "component": "minecraft:custom_name",
      "on_true": {
        "type": "minecraft:select",
        "model": "minecraft:item/shield",
        "property": "minecraft:component",
        "component": "minecraft:custom_name",
        "cases": [
          {
            "when": "Round Shield",
            "model": {
              "type": "minecraft:model",
              "model": "minecraft:item/excalibur/custom_name/shield/round/excalibur_shield"
            }
          },
          {
            "when": "Shield",
            "model": {
              "type": "minecraft:model",
              "model": "minecraft:item/excalibur/custom_name/shield/vanilla/excalibur_shield"
            }
          }
        ],
        "fallback": {
          "type": "minecraft:special",
          "base": "minecraft:item/shield",
          "model": {
            "type": "minecraft:shield"
          }
        }
      },
      "on_false": {
        "type": "minecraft:special",
        "base": "minecraft:item/shield",
        "model": {
          "type": "minecraft:shield"
        }
      }
    },
    "on_true": {
      "type": "minecraft:condition",
      "property": "minecraft:has_component",
      "component": "minecraft:custom_name",
      "on_true": {
        "type": "minecraft:select",
        "model": "minecraft:item/shield_blocking",
        "property": "minecraft:component",
        "component": "minecraft:custom_name",
        "cases": [
          {
            "when": "Round Shield",
            "model": {
              "type": "minecraft:model",
              "model": "minecraft:item/excalibur/custom_name/shield/round/excalibur_shield_blocking"
            }
          },
          {
            "when": "Shield",
            "model": {
              "type": "minecraft:model",
              "model": "minecraft:item/excalibur/custom_name/shield/vanilla/excalibur_shield_blocking"
            }
          }
        ],
        "fallback": {
          "type": "minecraft:special",
          "base": "minecraft:item/shield_blocking",
          "model": {
            "type": "minecraft:shield"
          }
        }
      },
      "on_false": {
        "type": "minecraft:special",
        "base": "minecraft:item/shield_blocking",
        "model": {
          "type": "minecraft:shield"
        }
      }
    },
    "property": "minecraft:using_item"
  }
}
